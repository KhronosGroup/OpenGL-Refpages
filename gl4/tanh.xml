    <refnamediv>
        <refname>tanh</refname>
        <refpurpose>return the hyperbolic tangent of the parameter</refpurpose>
    </refnamediv>
    <refsynopsisdiv><title>Declaration</title>
        <funcsynopsis>
            <funcprototype>
                <funcdef>genType <function>tanh</function></funcdef>
                <paramdef>genType <parameter>x</parameter></paramdef>
            </funcprototype>
        </funcsynopsis>
    </refsynopsisdiv>
    <refsect1 xml:id="parameters"><title>Parameters</title>
        <variablelist>
        <varlistentry>
            <term><parameter>x</parameter></term>
            <listitem>
                <para>
                    Specify the value whose hyperbolic tangent to return.
                </para>
            </listitem>
        </varlistentry>
        </variablelist>
